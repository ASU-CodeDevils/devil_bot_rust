{
  "name": "devil-bot-rust-cdk",
  "version": "0.1.0",
  "bin": {
    "devil-bot-rust-cdk": "bin/devil-bot-rust-cdk.js"
  },
  "scripts": {
    "prettier": "prettier --write './@(lib)/**/*@(.ts|.tsx)'",
